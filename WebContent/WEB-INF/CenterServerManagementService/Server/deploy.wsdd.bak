<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CenterServerManagementService WSDL service -->

  <service name="CenterServerManagement" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Server"/>
      <parameter name="wsdlServiceElement" value="CenterServerManagementService"/>
      <parameter name="schemaQualified" value="http://Server"/>
      <parameter name="wsdlServicePort" value="CenterServerManagement"/>
      <parameter name="className" value="Server.CenterServerManagementSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CenterServerManagement"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getRecordCounts" qname="operNS:getRecordCounts" xmlns:operNS="http://Server" returnQName="retNS:getRecordCountsReturn" xmlns:retNS="http://Server" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:managerId" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="transferRecord" qname="operNS:transferRecord" xmlns:operNS="http://Server" returnQName="retNS:transferRecordReturn" xmlns:retNS="http://Server" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:managerId" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:recordID" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:remoteCenterServerName" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="editRecord" qname="operNS:editRecord" xmlns:operNS="http://Server" returnQName="retNS:editRecordReturn" xmlns:retNS="http://Server" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:managerId" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:recordID" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:filedname" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:newValue" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="createTRecord" qname="operNS:createTRecord" xmlns:operNS="http://Server" returnQName="retNS:createTRecordReturn" xmlns:retNS="http://Server" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:managerId" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:firstName" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lastName" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:address" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:phone" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:specialization" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:location" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="createSRecord" qname="operNS:createSRecord" xmlns:operNS="http://Server" returnQName="retNS:createSRecordReturn" xmlns:retNS="http://Server" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:managerId" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:firstName" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lastName" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:courseRegistered" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:status" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:statusdate" xmlns:pns="http://Server" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getRecordCounts editRecord createSRecord transferRecord createTRecord"/>

  </service>
</deployment>
